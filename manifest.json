{
  "manifest_version": 2,
  "name": "True age for Trello card",
  "short_name": "Truello",
  "version": "1.1.0",
  "description": "Show how long the card has been in the Trello list",
  "author" : "Derrick Qin",
  "permissions": [
    "tabs",
    "https://trello.com/*"
  ],
  "content_scripts": [ {
    "js": [ "thirdPartyLib/moment.js", "truello.js" ],
    "matches": [ "https://trello.com/*" ]
  } ]
}
